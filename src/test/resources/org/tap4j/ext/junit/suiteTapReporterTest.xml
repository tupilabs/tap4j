<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
<suite name="Tap generation test by suite and group" configfailurepolicy="skip" parallel="false" junit="true">
  <listeners>
    <listener class-name="org.tap4j.ext.testng.SuiteTAPReporter" /> 
    <listener class-name="org.tap4j.ext.testng.TestTAPReporter" /> 
  </listeners>
  <test name="Tap generation test by suite and group for JUnit tests" preserve-order="true" parallel="false">
    <classes>
       <class name="org.tap4j.ext.junit.TestTap13JUnit1" />
       <class name="org.tap4j.ext.junit.TestTap13JUnit2" />
    </classes>	
  </test>
</suite>